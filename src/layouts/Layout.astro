---
import BaseLayout from '@/layouts/BaseLayout.astro'

export interface Props {
	title: string;
  description?: string;
  og_image?: string;
  path?: string;
}

const { title, og_image, description, path } = Astro.props;
---
<BaseLayout title={title} description={description} og_image={og_image} path={path}>
  <slot />
</BaseLayout>
